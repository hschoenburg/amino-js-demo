{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "sourceMap": true,
    "target": "es5",
    "lib": ["es2017", "dom"],
    "moduleResolution": "node",
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "esModuleInterop": true,
    //"noUnusedLocals": true,
  }
}
